{
  "name": "seniordesign",
  "version": "1.0.0",
  "private": true,
  "description": "A node app for finding the perfect outfit for every occasion",
  "scripts": {
    "start": "node ./app/bin/www",
    "debug": "DEBUG=express:* node ./app/bin/www",
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "mocha ./app/test/mocha --timeout=3000",
    "integration": "mocha ./app/test/integration",
    "coverage": "nyc npm run test",
    "nw": "nightwatch -e chrome",
    "nwff": "nightwatch -e firefox",
    "nww": "node nightwatch.js -e chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liampalmer0/SeniorDesign.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/liampalmer0/SeniorDesign/issues"
  },
  "homepage": "https://github.com/liampalmer0/SeniorDesign#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "pug": "^3.0.2",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^1.13.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chromedriver": "^89.0.0",
    "eslint": "^7.17.0",
    "geckodriver": "^1.21.1",
    "mocha": "^8.2.1",
    "nightwatch": "^1.5.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.4"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "pugTabWidth": 2,
    "pugAttributeSeparator": "as-needed"
  },
  "nyc": {
    "exclude": [
      "sequelize/**",
      "test/**",
      "*.js"
    ]
  }
}
